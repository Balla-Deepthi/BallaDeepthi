=IF(A2="1-50 employees", 10, IF(A2="51-200 employees", 20, IF(A2="201-1000 employees", 30, IF(A2="1000+ employees", 40, 0)))) + 
IF(B2="Less than $10,000", 10, IF(B2="$10,000 - $50,000", 20, IF(B2="$50,001 - $100,000", 30, IF(B2="More than $100,000", 40, 0)))) + 
IF(C2="Technology", 40, IF(C2="Finance", 30, IF(C2="Healthcare", 20, IF(C2="Retail", 10, 5)))) + 
IF(D2="Immediate (within 1 month)", 40, IF(D2="Short-term (1-3 months)", 30, IF(D2="Medium-term (3-6 months)", 20, 10)))
